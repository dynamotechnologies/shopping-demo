<p id="notice"><%= notice %></p>

<h1>Security Questions</h1>

<table>
  <thead>
    <tr>
      <th>Question</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @security_questions.each do |security_question| %>
      <tr>
        <td><%= security_question.question %></td>
        <td><%= link_to 'Show', security_question %></td>
        <td><%= link_to 'Edit', edit_security_question_path(security_question) %></td>
        <td><%= link_to 'Destroy', security_question, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Security Question', new_security_question_path %>
